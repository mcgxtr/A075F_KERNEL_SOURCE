			/* Fixed 5V,0.5A */
			/*HS07 code for HS07-752 by xiongxiaoliang at 20250415 start*/
			pd,source-pdo-data = <0x00019078>;
			/*HS07 code for HS07-752 by xiongxiaoliang at 20250415 end*/
			/*HS07 code for SR-AL7761A-01-20 by lina at 20250415 start*/
			pd,sink-pdo-size = <3>;
			/*HS07 code for HS07-2435 by xiongxiaoliang at 20250429 start*/
			pd,sink-pdo-data = <0x360190c8 0x0002d0a7 0xC0B43237>;
			/*HS07 code for HS07-2435 by xiongxiaoliang at 20250429 end*/
			/*HS07 code for SR-AL7761A-01-20 by lina at 20250415 end*/
			/*HS07 code for SR-AL7761A-01-20 by xiongxiaoliang at 20250319 end*/
			/*0110 1110 0011 0010
			 * No DP, host + device
			 *	pd,id-vdo-size = <6>;
			 *	pd,id-vdo-data = <0xd14029cf 0x0 0x17110000
						  0x61000000 0x0 0x41000000>;
