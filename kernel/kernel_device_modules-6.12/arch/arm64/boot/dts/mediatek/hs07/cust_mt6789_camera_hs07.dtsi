	    pins_cmd_dat {
			pinmux = <PINMUX_GPIO158__FUNC_GPIO158>;/*GPIO_DISP_ENP_PIN*/
			slew-rate = <1>;
			output-low;
		};
	};
	flashlight_pins_enf_gpio_high: enf_gpio@1 {
	    pins_cmd_dat {
			pinmux = <PINMUX_GPIO158__FUNC_GPIO158>;/*GPIO_DISP_ENP_PIN*/
			slew-rate = <1>;
			output-high;
		};
	};
};
