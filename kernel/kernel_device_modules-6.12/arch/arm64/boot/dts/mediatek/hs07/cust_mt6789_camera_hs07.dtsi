};

&pio {
	flashlight_pins_default: flashlight_default {
	};
	/*torch_en*/
	flashlight_pins_enm_gpio_low: enm_gpio@0 {
	    pins_cmd_dat {
			pinmux = <PINMUX_GPIO159__FUNC_GPIO159>;/*GPIO_FL_ENM_PIN*/
			slew-rate = <1>; /*direction 0:in, 1:out*/
			output-low;/*direction out used only. output_low or high*/
		};
	};

	flashlight_pins_enm_gpio_high: enm_gpio@1 {
	    pins_cmd_dat {
			pinmux = <PINMUX_GPIO159__FUNC_GPIO159>;/*GPIO_DISP_ENN_PIN*/
			slew-rate = <1>; /*direction 0:in, 1:out*/
			output-high;/*direction out used only. output_low or high*/
		};
	};
	/* HS07 V code for HS07-19  by zhongbin at 2025/03/11 start */
	flashlight_pins_enm_pwm_low: enm_pwm@0 {
	    pins_cmd_dat {
			pinmux = <PINMUX_GPIO159__FUNC_PWM_2>;/*GPIO_DISP_ENP_PIN*/
			slew-rate = <1>;
			bias-disable;
		};
	};
	flashlight_pins_enm_pwm_high: enm_pwm@1 {
	    pins_cmd_dat {
			pinmux = <PINMUX_GPIO159__FUNC_PWM_2>;/*GPIO_DISP_ENP_PIN*/
			slew-rate = <1>;
			bias-disable;
		};
	};
    /* HS07 V code for HS07-19  by zhongbin at 2025/03/11 end */
